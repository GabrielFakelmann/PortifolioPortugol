/* DESENVOLVIDO POR: Gabriel Fakelmann
  O algoritmo recebE um número inteiro e exibE o seu sucessor. 
*/

programa {
  funcao inicio() {
    inteiro num, contador
    escreva("Digite um número inteiro: ")
    leia(num)
    limpa()

    contador = num + 1

    escreva("Sucessor do número digitado é: ", contador)
  }
}
